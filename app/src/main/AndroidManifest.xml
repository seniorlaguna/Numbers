<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.seniorlaguna.numbers">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="org.seniorlaguna.numbers.LogoActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.seniorlaguna.numbers.MenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.seniorlaguna.numbers.OriginalGameActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.seniorlaguna.numbers.ReverseGameActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.seniorlaguna.numbers.GameoverActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="org.seniorlaguna.numbers.HighscoreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="org.seniorlaguna.numbers.SurviveGameActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>